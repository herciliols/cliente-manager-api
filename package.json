    "build": "npm run prebuild && npx tsc",
    "clean": "rimraf dist",
    "prebuild": "npm run clean"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/herciliols/cliente-manager-api.git"
  },
  "keywords": [
    "customer",
    "management",
    "application",
    "Node.js",
    "express",
    "mysql2"
  ],
  "author": "Hercílio Luz Simões",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/herciliols/cliente-manager-api/issues"
  },
  "homepage": "https://github.com/herciliols/cliente-manager-api#readme",
  "dependencies": {
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.9.1",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.22",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/validator": "^13.11.9",
    "dotenv": "^16.4.5",
    "module-alias": "^2.2.3",
    "nodemon": "^3.1.0",
    "swagger-autogen": "^2.23.7",
    "swagger-ui-express": "^5.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
