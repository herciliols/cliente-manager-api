{
  "name": "cliente-manager-api",
  "version": "1.0.0",
  "description": "",
  "main": "src/app.js",
  "scripts": {
    "start": "ts-node -r module-alias/register -r dotenv/config swagger-autogen.js && nodemon -r dotenv/config src/app.ts",
